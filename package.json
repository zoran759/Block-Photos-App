{
  "name": "react-photos",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "npm run build && npm run electron",
    "serve": "webpack-dev-server",
    "build": "webpack -p",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      }
    }
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "blockstack": "17.2.0",
    "cors": "^2.8.4",
    "electron-compile": "^6.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@ionic/core": "4.0.0-alpha.7",
    "@types/react": "16.4.1",
    "ajv": "6.5.1",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "electron": "^2.0.3",
    "electron-forge": "^4.1.7",
    "electron-prebuilt-compile": "1.8.2-beta.3",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "json-loader": "^0.5.7",
    "path": "^0.12.7",
    "style-loader": "^0.21.0",
    "url-loader": "1.0.1",
    "webpack": "3.12.0",
    "webpack-dev-server": "^2.4.2"
  },
  "babel": {
    "presets": [
      "react",
      "babel-preset-es2015"
    ]
  }
}
